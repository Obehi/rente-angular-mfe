{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "rente-front-end": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "rente",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "i18n": {
        "sourceLocale": {
          "code": "nb",
          "baseHref": ""
        },
        "locales": {
          "sv": {
            "translation": "src/locale/messages.sv.xtb",
            "baseHref": ""
          }
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "aot": true,
            "localize": true,
            "outputPath": "dist/rente-front-end",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/sitemap.xml",
              "src/env-config.json"
            ],
            "styles": [
              "src/styles.scss",
              "src/styles/rente-material-theme.scss"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ]
            },
            "nb": {
              "localize": ["nb"],
              "assets": [
                {
                  "glob": "**/*",
                  "input": "src/assets/nb",
                  "output": "/assets/"
                },
                {
                  "glob": "sitemap.xml",
                  "input": "src/sitemaps/no",
                  "output": "/"
                },
                "src/assets",
                "src/favicon.ico",
                "src/sitemap.xml",
                "src/robots.txt",
                "src/env-config.js"

              ],
              "fileReplacements": [
                {
                  "replace": "src/app/local-components/components-output.ts",
                  "with": "src/app/local-components/components-no.ts"
                },
                {
                  "replace": "src/index.html",
                  "with": "src/index/no/index-no.html"
                },
                {
                  "replace": "src/app/config/locale/locale.ts",
                  "with": "src/app/config/locale/locale-nb.ts"
                },
                {
                  "replace": "src/app/config/locale/locale-js.js",
                  "with": "src/app/config/locale/locale-nb.js"
                },
                {
                  "replace": "src/app/shared/constants/mask.ts",
                  "with": "src/app/shared/constants/mask-no.ts"
                },
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.heroku.ts"
                }
              ]
            },
            "no-local": {
              "localize": ["nb"],
              "assets": [
                {
                  "glob": "**/*",
                  "input": "src/assets/nb",
                  "output": "/assets/"
                },
                {
                  "glob": "sitemap.xml",
                  "input": "src/sitemaps/no",
                  "output": "/"
                },
                "src/assets",
                "src/favicon.ico",
                "src/sitemap.xml",
                "src/robots.txt",
                "src/env-config.js"

              ],
              "fileReplacements": [
                {
                  "replace": "src/app/local-components/components-output.ts",
                  "with": "src/app/local-components/components-no.ts"
                },
                {
                  "replace": "src/index.html",
                  "with": "src/index/no-dev/index-no-dev.html"
                },
                {
                  "replace": "src/app/config/locale/locale.ts",
                  "with": "src/app/config/locale/locale-nb.ts"
                },
                {
                  "replace": "src/app/config/locale/locale-js.js",
                  "with": "src/app/config/locale/locale-nb.js"
                },
                {
                  "replace": "src/app/shared/constants/mask.ts",
                  "with": "src/app/shared/constants/mask-no.ts"
                },
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.heroku.ts"
                }
              ]
            },
            "sv": {
              "localize": ["sv"],
              "assets": [
                {
                  "glob": "**/*",
                  "input": "src/assets/sv",
                  "output": "/assets/"
                },
                {
                  "glob": "sitemap.xml",
                  "input": "src/sitemaps/sv/",
                  "output": "/"
                },
                {
                  "glob": "index.html",
                  "input": "src/index/sv/",
                  "output": "/"
                },

                "src/assets",
                "src/favicon.ico",
                "src/sitemap.xml",
                "src/robots.txt",
                "src/env-config.js"

              ],
              "fileReplacements": [
                {
                  "replace": "src/app/local-components/components-output.ts",
                  "with": "src/app/local-components/components-sv.ts"
                },
                {
                  "replace": "src/app/config/locale/locale-js.js",
                  "with": "src/app/config/locale/locale-sv.ts"
                },
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.heroku.ts"
                },
                {
                  "replace": "src/app/shared/constants/mask.ts",
                  "with": "src/app/shared/constants/mask-sv.ts"
                }
              ]
            },
            "sv-local": {
              "localize": ["sv"],
              "assets": [
                {
                  "glob": "**/*",
                  "input": "src/assets/sv",
                  "output": "/assets/"
                },
                {
                  "glob": "sitemap.xml",
                  "input": "src/sitemaps/sv/",
                  "output": "/"
                },
                {
                  "glob": "index.html",
                  "input": "src/index/sv-dev/",
                  "output": "/"
                },
                "src/assets",
                "src/favicon.ico",
                "src/sitemap.xml",
                "src/robots.txt",
                "src/env-config.js"
              ],
              "fileReplacements": [
                {
                  "replace": "src/app/local-components/components-output.ts",
                  "with": "src/app/local-components/components-sv.ts"
                },
                {
                  "replace": "src/index.html",
                  "with": "src/index/sv-dev/index-sv-dev.html"
                },
                {
                  "replace": "src/app/config/routes-config.ts",
                  "with": "src/app/config/routes-config-sv.ts"
                },
                {
                  "replace": "src/app/config/locale/locale-js.js",
                  "with": "src/app/config/locale/locale-sv.ts"
                },
                {
                  "replace": "src/app/shared/constants/mask.ts",
                  "with": "src/app/shared/constants/mask-sv.ts"
                },
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.heroku.ts"
                }
              ]
            },
            "local": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.local.ts"
                }
              ]
            },
            "develop": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.develop.ts"
                }
              ]
            },
            "heroku-sv": {
              "index": {
                "input": "src/index/sv/index-sv.html",
                "output": "index.html"
              },
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.heroku.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ]
            },
            "heroku-dev": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.heroku.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ]
            },
            "heroku-prod": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.heroku.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ]
            },
            "heroku": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.heroku.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ]
            },
            "sv": {
              "aot": true,
              "outputPath": "dist/rente-front-end-sv",
              "i18nFile": "src/i18n/messages.sv.xlf",
              "i18nFormat": "xlf",
              "i18nLocale": "sv"
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "aot": true,
            "browserTarget": "rente-front-end:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "rente-front-end:build:production"
            },
            "sv": {
              "browserTarget": "rente-front-end:build:sv"
            },
            "nb": {
              "browserTarget": "rente-front-end:build:nb"
            },
            "local": {
              "browserTarget": "rente-front-end:build:local"
            },
            "develop": {
              "browserTarget": "rente-front-end:build:develop"
            },
            "sv": {
              "browserTarget": "rente-front-end:build:sv"
            },
            "sv-local": {
              "browserTarget": "rente-front-end:build:sv-local"
            },
            "no-local": {
              "browserTarget": "rente-front-end:build:no-local"
            }
          },
          "i18nFile": "src/locale/messages.nb.xtb",
          "i18nFormat": "xtb",
          "i18nLocale": "nb",
          "i18nMissingTranslation": "error",
          "baseHref": "/nb/",
          "i18n": {
            "sourceLocale": "nb",
            "locales": {
              "nb": "src/locale/messages.nb.xtb"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "rente-front-end:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": ["src/styles.scss"],
            "scripts": [],
            "assets": ["src/favicon.ico", "src/assets"]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["src/tsconfig.app.json", "src/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "rente-front-end-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "rente-front-end:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "rente-front-end:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    }
  },
  "defaultProject": "rente-front-end",
  "cli": {
    "analytics": false
  }
}
