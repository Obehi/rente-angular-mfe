{
    "extends": "../tslint.json",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "rente",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "rente",
            "kebab-case"
        ],
        "max-line-length": [
            false
        ],
        "whitespace": false,
        "typedef-whitespace": false
    }
}
