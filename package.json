{
  "name": "rente-front-end",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "cd server && node app.js",
    "build": "npm run version && npm run i18n-poeditor",
    "build:stat": "npm run lint && npm run version && npm run i18n && npm run build-i18n:stats",
    "local": "ng serve --configuration=local",
    "no-local": "ng serve --configuration=no-local",
    "test": "ng test",
    "lint": "tsc --noEmit && eslint . --ext js,ts,json --quiet --fix",
    "e2e": "ng e2e",
    "i18n": "ng xi18n --i18n-format=xmb --out-file rente-i18n.xmb --i18n-locale en",
    "build-i18n:no": "ng build --output-path=dist/rente-front-end/ --aot --prod --sourceMap --i18n-file=src/locale/messages.no.xtb --i18n-format=xtb --i18n-locale=nn",
    "build-i18n:no:stats": "ng build --stats-json --output-path=dist/rente-front-end/ --aot --prod --sourceMap --i18n-file=src/locale/messages.no.xtb --i18n-format=xtb --i18n-locale=nn",
    "build-i18n:en": "ng build --output-path=dist/rente-front-end/en/ --aot --prod --sourceMap --base-href /en/ --i18n-file=src/locale/messages.en.xtb --i18n-format=xtb --i18n-locale=en",
    "build-i18n:en:stats": "ng build --stats-json --output-path=dist/rente-front-end/en/ --aot --prod --sourceMap --base-href /en/ --i18n-file=src/locale/messages.en.xtb --i18n-format=xtb --i18n-locale=en",
    "build-i18n": "npm run build-i18n:no",
    "build-i18n:stats": "npm run build-i18n:no:stats && npm run build-i18n:en:stats",
    "version": "node app-version.js",
    "heroku-postbuild": "node post-install.js",
    "config-env": "ts-node ./src/set-env.ts",
    "set-local-components": "ts-node ./src/set-local-components.ts",
    "build:stats-analyze": "ng build --stats-json",
    "analyze": "webpack-bundle-analyzer dist/rente-front-end/stats.json",
    "i18n:poeditor": "poeditor --config=poeditor.json",
    "i18n-poeditor": "ng xi18n --i18nFormat=xmb --out-file messages.xmb && npm run i18n:poeditor",
    "move-test": "ng build --configuration=heroku-dev,sv && node move-locale-dir.js",
    "set-local-variable": "ts-node ./src/set-local-variable.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.0.3",
    "@angular/cdk": "^8.0.1",
    "@angular/common": "~10.0.3",
    "@angular/compiler": "~10.0.3",
    "@angular/core": "^10.0.3",
    "@angular/forms": "~10.0.3",
    "@angular/localize": "^10.0.8",
    "@angular/material": "^8.0.1",
    "@angular/platform-browser": "~10.0.3",
    "@angular/platform-browser-dynamic": "~10.0.3",
    "@angular/router": "~10.0.3",
    "@ng-bootstrap/ng-bootstrap": "^8.0.0",
    "angular": "^1.7.8",
    "angular-highcharts": "^9.0.11",
    "angular-imask": "^6.0.5",
    "angular-popper": "^2.0.1",
    "bootstrap": "^4.5.0",
    "core-js": "^2.5.4",
    "dotenv": "^8.1.0",
    "express": "^4.16.3",
    "fs": "0.0.1-security",
    "highcharts": "^8.1.2",
    "http-proxy": "^1.18.0",
    "https": "^1.0.0",
    "mat-select-autocomplete": "^1.3.0",
    "net": "^1.0.2",
    "ng-drift": "^0.1.1",
    "ng-lazyload-image": "9.0.0",
    "ng2-currency-mask": "^9.0.2",
    "ngx-countup": "7.3.0",
    "poeditor-connector": "1.0.2",
    "popper.js": "^1.16.1",
    "rxjs": "~6.5.2",
    "smoothscroll-polyfill": "^0.4.4",
    "sockjs-client": "^1.4.0",
    "ssl-root-cas": "^1.3.1",
    "stompjs": "^2.3.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.0",
    "@angular/cli": "~10.0.0",
    "@angular/compiler-cli": "^10.0.3",
    "@angular/language-service": "~10.0.3",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "codelyzer": "^5.1.2",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "prettier": "^2.2.1",
    "prettier-eslint": "^12.0.0",
    "protractor": "~7.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.5",
    "webpack-bundle-analyzer": "^3.8.0"
  }
}
