{
  "name": "rente-front-end",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "cd server && node app.js",
    "build": "npm run lint && npm run version && npm run i18n && npm run build-i18n",
    "local": "ng serve --configuration=local",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "npm run build",
    "i18n": "ng xi18n --i18n-format=xmb --out-file rente-i18n.xmb --i18n-locale en",
    "build-i18n:no": "ng build --output-path=dist/rente-front-end/ --aot --prod --sourceMap --i18n-file=src/locale/messages.no.xtb --i18n-format=xtb --i18n-locale=nn",
    "build-i18n:en": "ng build --output-path=dist/rente-front-end/en/ --aot --prod --sourceMap --base-href /en/ --i18n-file=src/locale/messages.en.xtb --i18n-format=xtb --i18n-locale=en",
    "build-i18n": "npm run build-i18n:no && npm run build-i18n:en",
    "version": "node app-version.js",
    "heroku-postbuild": "npm run config-env && ng build --configuration=heroku",
    "config-env": "ts-node ./src/set-env.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.0.3",
    "@angular/cdk": "^8.0.1",
    "@angular/common": "~8.0.2",
    "@angular/compiler": "~8.0.2",
    "@angular/core": "~8.0.2",
    "@angular/forms": "~8.0.2",
    "@angular/material": "^8.0.1",
    "@angular/platform-browser": "~8.0.2",
    "@angular/platform-browser-dynamic": "~8.0.2",
    "@angular/router": "~8.0.2",
    "angular2-text-mask": "^9.0.0",
    "core-js": "^2.5.4",
    "dotenv": "^8.1.0",
    "express": "^4.16.3",
    "net": "^1.0.2",
    "rxjs": "~6.5.2",
    "sockjs-client": "^1.3.0",
    "stompjs": "^2.3.3",
    "text-mask-addons": "^3.8.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.4",
    "@angular/cli": "~8.0.4",
    "@angular/compiler-cli": "~8.0.2",
    "@angular/language-service": "~8.0.2",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.4.5"
  }
}
